{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "wf-dashboard-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/wfDashboard/WfDashboardWebPart.js",
          "manifest": "./src/webparts/wfDashboard/WfDashboardWebPart.manifest.json"
        }
      ]
    },
    "quote-request-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/quoteRequest/QuoteRequestWebPart.js",
          "manifest": "./src/webparts/quoteRequest/QuoteRequestWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "WfDashboardWebPartStrings": "lib/webparts/wfDashboard/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "QuoteRequestWebPartStrings": "lib/webparts/quoteRequest/loc/{locale}.js"
  }
}
